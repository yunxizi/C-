\\\\两个数的加法\\\\\
#include(stdio.h)
int main()
{
  int sum1=0;
  int sum2=0;
  int sum=0;
  scanf("%d%d",&sum1,&sum2);
  sum=sum1+sum2;
  printf("%d\n",sum);
return 0;
}

\\计算数据类型长度
#include(stdio.h)
int mian()
{
  printf("%d\n",sizeof(int));\\\\计算int所占字节长度
  return 0;
}

\\全局变量作用于整个工程，局部变量作用于括号内。   {生命周期类似}

#include<stdio.h>
#include<stafx.h>
#include<math.h>
int main()
{
 printf("%f",5*sin(3.14/3)+12.5*3.4+sqart(16.88)); //打印5乘以sin60+12.5乘以3.4+根号下16.88的结果。
return 0;
}
